#include <fftw3.h>

#define SIZE 512

int main() {
    double in[SIZE];
    fftw_complex out[SIZE];

    fftw_plan plan = fftw_plan_dft_1d(SIZE,
                                   &in,
                                   &out,
                                   FFTW_FORWARD,
                                   FFTW_ESTIMATE);

    fftw_execute(plan);

    fftw_destroy_plan(plan);
    fftw_free(in);
    fftw_free(out);

    return 0;
}
